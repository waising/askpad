<?xml version="1.0" encoding="utf-8"?>
<com.asking.pad.app.widget.FoursquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                    xmlns:app="http://schemas.android.com/apk/res-auto"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="match_parent">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="15dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="@dimen/space_5">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"
            android:src="@mipmap/ic_class_unfinish"/>

    </android.support.v7.widget.CardView>

</com.asking.pad.app.widget.FoursquareRelativeLayout>